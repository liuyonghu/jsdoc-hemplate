{
  "name": "jsdoc-hemplate",
  "version": "0.1.1",
  "description": "A clean, responsive documentation template theme for JSDoc 3 inspired by lodash and minami",
  "main": "publish.js",
  "scripts": {
    "test": "jsdoc -c fixtures/fixtures.conf.json",
    "sync": "browser-sync start -s ../fixtures-doc -f ../fixtures-doc --reload-delay 1000 --no-ui --no-notify",
    "watch": "watch-run -d 1000 -p tmpl/**,static/** \"npm run test\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liuyonghu/jsdoc-hemplate.git"
  },
  "devDependencies": {
    "browser-sync": "latest",
    "jsdoc": "latest",
    "watch-run": "latest"
  },
  "author": "liuyonghu",
  "license": "Apache-2.0",
  "keywords": [
    "jsdoc",
    "template"
  ],
  "files": [
    "publish.js",
    "static",
    "tmpl"
  ],
  "dependencies": {
    "@jsdoc/salty": "^0.2.1"
  }
}
